#!/usr/bin/env gnuplot

set boxwidth 0.5
set style fill solid
set xlabel "Policy"
set ylabel "Priority * Missed Deadline"
unset key
#set terminal svg size 1024,768
#set output "hetero.svg"
set xtics rotate by -10 left offset -4,0
set xtics font ", 12" 
set terminal postscript eps enhanced color
set output "hetero.eps"
plot "hetero.dat" using 1:3:xtic(2) with boxes, "hetero.dat" using 1:($3+500000):3 with labels


